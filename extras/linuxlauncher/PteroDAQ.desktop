[Desktop Entry]
Type=Application
Version=1.0
Name=PteroDAQ
GenericName=Data Acquisition System
Icon=pterodaq
Exec=/usr/bin/env bash -c 'xdg-icon-resource install --size 512 --novendor "$(dirname "$0")"/../appicons/pterodaq512.png pterodaq; PYBIN=`type -P python3`; if [ -z $PYBIN ]; then PYBIN=`type -P python2`; fi; if [ -z $PYBIN ]; then PYBIN=`type -P python`; fi; if [ -z $PYBIN ]; then exit; fi; "$PYBIN" "$(dirname "$0")"/../../daq' %k
Terminal=false

